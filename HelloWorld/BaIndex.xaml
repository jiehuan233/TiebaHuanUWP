<Page
    x:Class="HelloWorld.BlankPage1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HelloWorld"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:Name="Ba_Name" HorizontalAlignment="Left" Height="67" Margin="138,-5,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="280" SelectionChanged="TextBlock_SelectionChanged" FontSize="50"/>
        <Button x:Name="back" Content="back" Height="48" Margin="10,7,0,0" VerticalAlignment="Top" Width="108" FontSize="25" Click="back_Click"/>
        <WebView x:Name="Ba_Page" HorizontalAlignment="Center" Height="696" VerticalAlignment="Top" Width="412" Margin="0,62,0,0"/>

    </Grid>
</Page>
